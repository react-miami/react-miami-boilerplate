{
  "name": "react-miami-boilerplate",
  "version": "1.0.0",
  "author": "marracheco@gmail.com",
  "description": "A React/Redux boilerplate with Bootstrap.",
  "private": false,
  "scripts": {
    "rest-server": "node server.babel.js",
    "start": "npm run build && webpack-dev-server --history-api-fallback --devtool eval-source-map --hot --inline --progress --colors",
    "build": "npm run clean && webpack --progress --colors",
    "build:prod": "npm run clean && NODE_ENV=production webpack --progress --colors",
    "build-old": "NODE_ENV=production && npm run clean && webpack -p",
    "clean": "rimraf build",
    "lint": "eslint ./",
    "git-update": "git update-index --again"
  },
  "precommit": {
    "run": "git-update",
    "silent": true
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-lodash": "^1.10.1",
    "eslint-plugin-react": "^6.1.1",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.5",
    "history": "^3.0.0",
    "ignore-styles": "^4.0.0",
    "immutable": "^3.8.1",
    "imports-loader": "^0.6.5",
    "jquery": "^3.1.0",
    "jsdom": "^9.4.2",
    "node-fetch": "^1.6.0",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.10.0",
    "pre-commit": "^1.1.3",
    "prunk": "^1.3.0",
    "query-string": "^4.2.3",
    "raw-loader": "^0.5.1",
    "react": "^15.3.0",
    "react-bootstrap": "^0.30.2",
    "react-dom": "^15.3.0",
    "react-global": "^0.1.8",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-require": "0.0.16"
  },
  "devDependencies": {}
}
